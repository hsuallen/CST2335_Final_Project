<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nothi.androidamenities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NearPrinterActivity">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearPrinterActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NearMaleWashroomActivity">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearMaleWashroomActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NearFemaleWashroomActivity">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearFemaleWashroomActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NearMicrowave">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearMicrowave" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NearVendingActivity">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearVendingActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NearestinCommons">
            <intent-filter>
                <action android:name="com.example.nothi.androidamenities.NearestinCommons" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>


        </activity>


    </application>

</manifest>