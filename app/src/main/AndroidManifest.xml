        <activity android:name=".NearMicrowave"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MicroDetails"></activity>
        <activity android:name=".NearVendingActivity"></activity>
        <activity android:name=".NearestinCommons"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".NearestinBusiness"
